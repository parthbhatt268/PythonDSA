    "# Sort in ascending\n",
    "a = [1,10, 3,4,6,7]\n",
    "\n",
    "print(\"Not Reverse: \",a[::1])\n",
    "print(\"Reverse: \",a[::-1])\n",
    "\n",
    "a.sort() # Ascending\n",
    "print(a)\n",
    "a.sort(reverse=True) # Descending\n",
    "print(a)\n"
